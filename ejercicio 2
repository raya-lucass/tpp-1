numero1 = 255
numero2 = 148
suma = numero1 + numero2
print("Suma:", suma)
resta = numero1 - numero2
print("Resta:", resta)
multiplicacion = numero1 * numero2
print("Multiplicación:", multiplicacion)
division = numero1 / numero2
print("División:", division)
modulo = numero1 % numero2
print("Módulo:", modulo)
print("Comparaciones:")
print("Suma > Resta:", suma > resta)
print("Resta == Multiplicación:", resta == multiplicacion)
print("Multiplicación < División:", multiplicacion < division)
print("División != Módulo:", division != modulo)
